<p id="notice"><%= notice %></p>

<h1>Listing Analysis Groups</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @analysis_groups.each do |analysis_group| %>
      <tr>
        <td><%= analysis_group.name %></td>
        <td><%= link_to 'Show', analysis_group %></td>
        <td><%= link_to 'Edit', edit_analysis_group_path(analysis_group) %></td>
        <td><%= link_to 'Destroy', analysis_group, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Analysis group', new_analysis_group_path %>
